<div class="container">
<%= form_with(model: @article , url: articles_path, method: 'post') do |f| %>
    <div>
        <%= f.label :title, 'タイトル' %>
    </div>
    <div>
        <%= f.text_field :title, class: 'text' %>
    </div>
    <div>
        <%= f.label :content, 'コンテンツ' %>
    </div>
    <div>
        <%= f.text_area :content %>
    </div>
        <%= f.submit '保存', class: 'btn-primary' %>
<% end %>
</div>